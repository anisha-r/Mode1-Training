package com.hcl.obj;

public class Employ {
	int empno;
	String name;
	double basic;
	
	public Employ[] showEmploy() {
		Employ[] arrEmploy=new Employ[4];
		
		arrEmploy[0]=new Employ();
		arrEmploy[0].empno=1;
		arrEmploy[0].name="anisha";
		arrEmploy[0].basic=83444;
		
		arrEmploy[1]=new Employ();
		arrEmploy[1].empno=1;
		arrEmploy[1].name="achuuu";
		arrEmploy[1].basic=83454;
		
		arrEmploy[2]=new Employ();
		arrEmploy[2].empno=1;
		arrEmploy[2].name="laduuu";
		arrEmploy[2].basic=83464;
		
		return arrEmploy;
	}
	public static void main(String[] args) { 
		
		Employ[] result=new Employ().showEmploy();
		for (Employ e : result) {
			System.out.println(e.empno + " "+e.name +" "+e.basic);
			
		}
		
	}

}
